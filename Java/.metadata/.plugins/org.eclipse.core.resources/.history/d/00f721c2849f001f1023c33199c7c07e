package com.java.oops;

public class Student {
//this class is for use of static and this keyword
	static String collegeName="DYPIET";
	
	int id;
	String name;
	String address;
	int fees;
	
	public Student(int id, String name, String address, int fees) {
		id= id;
		name= name;
		address= address;
		fees= fees;
	}
	
	
	
}
